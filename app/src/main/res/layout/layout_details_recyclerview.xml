<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView_detailsRecyclerView_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="2021-12-31"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginTop="18dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_detailsRecyclerView_date"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:padding="5dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_date" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:padding="5dp"
        android:text="Cash Out"
        android:textColor="#F4511E"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:padding="5dp"
        android:text="-10000.00"
        android:textColor="#F4511E"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_equel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:padding="5dp"
        android:text="="
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/textView_detailsRecyclerView_balance"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_operand2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:padding="5dp"
        android:text="1000.00"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/textView_detailsRecyclerView_equel"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_opcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:padding="5dp"
        android:text="/"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/textView_detailsRecyclerView_operand2"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

    <TextView
        android:id="@+id/textView_detailsRecyclerView_operand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:padding="5dp"
        android:text="100000.00"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/textView_detailsRecyclerView_opcode"
        app:layout_constraintTop_toBottomOf="@+id/textView_detailsRecyclerView_details" />

</androidx.constraintlayout.widget.ConstraintLayout>